    "tagged.js",
    "deps/",
    "middleware/",
    "plugins/",
    "deps",
    "middleware",
    "plugins"
  ],
  "directories": [
    "deps",
    "middleware",
    "plugins"
  ],
  "scripts": {
    "test": "node ./test/examples && node ./test/test",
